# App
APP_ENV=dev
APP_HOST=127.0.0.1
APP_PORT=8000

# Storage
DATA_DIR=./data

# SQLite (async)
DATABASE_URL=sqlite+aiosqlite:///./data/app.db

# Qdrant
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=

# Embeddings
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
EMBEDDING_DEVICE=gpu
EMBEDDING_BATCH_SIZE=32

# Timeouts
HTTP_TIMEOUT_S=60
